LINKER=@CMAKE_C_COMPILER@
LDFLAGS=@LDFLAGS@ -L@PLUMBER_PREFIX@/lib -lpstd -lproto -ljsonschema @ROCKSDB_LDFLAGS@
CFLAGS=@CFLAGS@ -I@CMAKE_CURRENT_SOURCE_DIR@/include -I@PLUMBER_PREFIX@/include/pstd -I@PLUMBER_PREFIX@/include/proto -I@PLUMBER_PREFIX@/include/jsonschema @ROCKSDB_CFLAGS@
OUTPUT=@CMAKE_CURRENT_BINARY_DIR@/bin
SRC=$(subst ;, ,@SERVLET_SOURCE@)
TARGET=@SERVLET_NAME@
